{
    "AWSEBDockerrunVersion": "1",
    "Image": {
      "Name": "dahaba/react-app",
      "Update": "true"
    },
    "Ports": [
      {
        "ContainerPort": "1234"
      }
    ],
    "Volumes": [
      {
        "HostDirectory": "/var/app/mydb",
        "ContainerDirectory": "/etc/mysql"
      }
    ],
    "Logging": "/var/log/nginx",
    "Entrypoint": "/app/bin/myapp",
    "Command": "--argument"
  }